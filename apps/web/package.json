{
	"name": "web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:e2e && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"check-format": "prettier --plugin-search-dir . --check . ",
		"format": "prettier --plugin-search-dir . --write .",
		"test:e2e": "DEBUG=pw:webserver playwright test",
		"test:unit": "vitest run",
		"db-types": "supabase gen types typescript --project-id gwdqcvmygjbnfhatthvi > src/lib/database/database.types.ts",
		"supa:start": "supabase start",
		"supa:stop": "supabase stop",
		"supa:restart": "supabase stop && supabase start",
		"db:dump": "supabase db dump --local --schema auth --data-only --file supabase/seed.sql --exclude auth.audit_log_entries && pnpm db:update-seed",
		"db:update-seed": "cat supabase/init-local.sql >> supabase/seed.sql",
		"db:reset": "supabase db reset --local"
	},
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.11.0",
		"@supabase/supabase-js": "^2.39.3"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-v8": "^1.2.2",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.4.5",
		"eslint": "^8.28.0",
		"eslint-config-custom": "workspace:*",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.31",
		"prettier": "~3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"supabase": "^1.150.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.1.3",
		"vitest": "^1.2.2"
	},
	"type": "module"
}
